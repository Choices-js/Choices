{
  "compilerOptions": {
    "module": "es6",
    "lib": ["es2017", "dom"],
    "target": "es5",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "strict": false,
    "noImplicitAny": false,
    "declaration": true,
    "allowJs": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "strictNullChecks": true,
    "newLine": "lf",
    "types": ["cypress", "node"],
    "declarationDir": "../public/types/cypress"
  },
  "include": ["."]
}